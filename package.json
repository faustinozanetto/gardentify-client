{
  "name": "@pankod/next-boilerplate",
  "description": "A well-structured production ready Next.js boilerplate with Typescript, Redux, Jest, React Testing Library, Express.js, Sass, Css, EnvConfig, Fetch, Reverse Proxy, Bundle Analyzer and Built-in Project CLI. https://pankod.github.io/next-boilerplate/",
  "version": "2.0.0",
  "repository": "https://github.com/pankod/next-boilerplate",
  "scripts": {
    "start:dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@next/bundle-analyzer": "^12.0.10",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "framer-motion": "6",
    "graphql": "^16.3.0",
    "http-proxy-middleware": "^2.0.3",
    "i18next-xhr-backend": "^3.2.2",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.0.10",
    "next-apollo": "^5.0.8",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^10.3.0",
    "next-routes": "^1.4.2",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.7",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/styled-components": "^5.1.22",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-eslint": "10.x",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-preset-react-app": "^10.0.1",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-ts": "^0.4.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "8.x",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "license": "ISC"
}
